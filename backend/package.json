{
  "name": "mqtt-backend-server",
  "version": "1.0.0",
  "description": "MQTT Backend Server for AWS IoT Core connection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "restart": "node restart-server.js",
    "restart-ps": "powershell -ExecutionPolicy Bypass -File restart-backend.ps1",
    "restart-force": "powershell -ExecutionPolicy Bypass -File restart-backend.ps1 -Force",
    "restart-auto": "powershell -ExecutionPolicy Bypass -File restart-backend.ps1 -Auto",
    "restart-linux": "bash restart-backend.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mqtt",
    "aws-iot",
    "websocket",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mqtt": "^5.3.5",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
